{
	"name": "@howells/falcon",
	"version": "1.0.2",
	"description": "fal.ai image generation CLI with interactive studio mode",
	"type": "module",
	"bin": {
		"falcon": "./bin/falcon"
	},
	"files": [
		"bin",
		"src",
		"package.json",
		"README.md"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/howells/falcon.git"
	},
	"homepage": "https://github.com/howells/falcon",
	"keywords": [
		"fal",
		"fal.ai",
		"image-generation",
		"ai",
		"cli",
		"gpt",
		"gemini"
	],
	"author": "Daniel Howells",
	"license": "MIT",
	"scripts": {
		"dev": "bun run src/index.ts",
		"build": "bun build src/index.ts --outdir dist --target node",
		"link": "bun link",
		"typecheck": "tsc --noEmit",
		"lint": "biome check --write . && ultracite",
		"prepare": "husky"
	},
	"lint-staged": {
		"*.{ts,tsx}": [
			"biome check --write",
			"ultracite"
		],
		"*.{js,json}": [
			"biome check --write"
		]
	},
	"dependencies": {
		"ink": "^5.0.1",
		"ink-text-input": "^6.0.0",
		"react": "^18.3.1",
		"commander": "^12.1.0",
		"chalk": "^5.3.0",
		"ora": "^8.0.1"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.11",
		"@types/bun": "latest",
		"@types/react": "^18.3.3",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.7",
		"react-devtools-core": "^7.0.1",
		"typescript": "^5.5.0",
		"ultracite": "^7.0.11"
	}
}
